# Welstory API Credentials
# Copy this file to .env and fill in your actual credentials

# Your Welstory username
WELSTORY_USERNAME=your_username_here

# Your Welstory password
WELSTORY_PASSWORD=your_password_here

# Server configuration (optional)
PORT=3000

# Base URL for Welstory API (optional, uses default if not set)
# WELSTORY_BASE_URL=https://welplus.welstory.com/

# Enable automatic token refresh (optional, defaults to true)
# WELSTORY_AUTO_REFRESH=true
